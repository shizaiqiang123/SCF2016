<?xml version="1.0" encoding="UTF-8"?>
<esbservice>
	<id>ESBS000025</id>
	<name>ftp服务端</name>
	<type>FTP_SERVER</type><!-- 对内开放的服务类型，消费者凭此类型调用服务 -->
	<protocol>127.0.0.1</protocol>
	<initlize>S</initlize>
	<protocoltp>FTP</protocoltp><!-- 接口协议类型 -->
	<port>1219</port>
	<inorout>o</inorout><!-- 当前服务是对内还是对外开放的，对内开放，属性对接外部接口，对外开放，属性对接内部监听 -->
	<timeout>60</timeout>
	<user>admin</user>
	<pwd>admin</pwd>
	<homeDir>D:\ftp</homeDir>
</esbservice>
