<?xml version="1.0" encoding="UTF-8"?>
<logicflow>
	<id>L_P_000577</id>
	<name>应收账款池融资-卖方还款-自定义逻辑流</name>
	<desc>应收账款池融资-卖方还款-自定义逻辑流</desc>
	<module></module>	
	<transfornode>
		<id>N00000001</id>
		<name>新增还款表</name>
		<desc>新增还款表</desc>
		<module>TRX</module>
		<event>TestEvent</event>
		<component>trxTransForToRecord</component>
		<fpParam>sysRefNo</fpParam>
		<tablename>TRX.LOAN_M</tablename>
		<doname>loan</doname>
		<type>E</type>
		<cascadeevent>Y</cascadeevent>
		<pending>
			<fields>sysLockFlag,sysLockBy</fields>
			<values>T,$sysLockBy$</values>
		</pending>
		<release>
			<fields>ttlLoanBal,intAmt,payIntAmt,payPrinAmt,pmtAmt</fields>
			<values>$ttlLoanBal$,$intAmt$,$payIntAmt$,$payPrinAmt$,$pmtAmt$</values>
		</release>
		<delepending>
			<fields>sysLockFlag</fields>
			<values>F</values>
		</delepending>
		<nodejs>L_P_000131.js</nodejs>
	</transfornode>
	<transfornode>
		<id>N00000002</id>
		<name>更新协议主表</name>
		<desc>归还协议额度</desc>
		<module>TRX</module>
		<event>TestEvent</event>
		<component>trxTransForToRecord</component>
		<tablename>TRX.CNTRCT_M</tablename>
		<fpParam>sysRefNo</fpParam>
		<type>E</type>
		<cascadeevent>Y</cascadeevent>
		<pending>
			<fields>sysLockFlag,sysLockBy</fields>
			<values>T,$sysLockBy$</values>
		</pending>
		<release>
			<fields>paySts,lmtBal,openLoanAmt,arAvalLoan</fields>
			<values>2,$lmtBal$,$ttlLoanBal$,$arAvalLoan$</values>
		</release>
		<delepending>
			<fields>sysLockFlag</fields>
			<values>F</values>
		</delepending>
		<nodejs>L_P_000056N00000006.js</nodejs>
	</transfornode>	
	<lnode>
		<id>N00000003</id>
		<name>TEST 00001</name>
		<desc>DESC TEST1</desc>
		<module>TEST</module>
		<event>TestEvent</event>
		<component>trxEditDoRecord</component>
		<tablename>TRX.INT_M</tablename>
		<doname>int</doname>
		<fpParam>sysRefNo</fpParam>
		<condition>theirRef=?</condition>
		<params>theirRef</params>
		<type>E</type>
		<cascadeevent>Y</cascadeevent>
		<nodejs>L_S_000001.js</nodejs>
	</lnode>
	<transfornode>
		<id>N00000004</id>
		<name>更新卖方额度表</name>
		<desc>更新卖方额度表</desc>
		<module>TEST</module>
		<event>TestEvent</event>
		<component>trxTransForToRecord</component>
		<tablename>TRX.LMT_M</tablename>
		<fields></fields>
		<doname>sellerLmt</doname>
		<type>E</type>
		<cascadeevent>Y</cascadeevent>
		<pending>
			<fields>sysLockFlag,sysLockBy</fields>
			<values>T,$sysLockBy$</values>
		</pending>
		<release>
			<fields>lmtBal,lmtAllocate,lmtRecover</fields>
			<values>$lmtBal$,$lmtAllocate$,$lmtRecover$</values>
		</release>
		<delepending>
			<fields>sysLockFlag</fields>
			<values>F</values>
		</delepending>
		<nodejs>L_S_000001.js</nodejs> 
	</transfornode>	
</logicflow>