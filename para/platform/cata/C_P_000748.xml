<!-- 	<condition><![CDATA[sysRefNo like %$sysRefNo$% and selNm like %$selNm$%  and selId like %$selId$% and cntrctNo like %$cntrctNo$% and patnerNm like %$patnerNm$% and storageNm like %$storageNm$%  and busiTp=4 and  sysLockFlag = F and  sysTrxSts = M and lmtBal > 0 and cntrctSts <> 1 and sysBusiUnit =$sysBusiUnit$]]></condition> -->
	<condition><![CDATA[a.sys_Ref_No like '%$sysRefNo$%' and a.sel_Nm like '%$selNm$%'  and a.sel_Id like '%$selId$%' and a.cntrct_No like '%$cntrctNo$%'  and a.busi_Tp=10 and  a.sys_Lock_Flag = 'F' and  a.sys_Trx_Sts = 'M' and a.lmt_Bal > 0 and a.sys_Busi_Unit ='$sysBusiUnit$' and b.collat_qty_bal >0 and a.lmt_Due_Dt >= to_date('$sysCurrentDate$','yyyy-MM-dd') and a.lmt_Valid_Dt <= to_date('$sysCurrentDate$','yyyy-MM-dd') and a.sys_Org_Id = $sysOrgId$ and b.sys_Org_Id = $sysOrgId$]]></condition>
