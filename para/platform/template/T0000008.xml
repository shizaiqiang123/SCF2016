<?xml version="1.0" encoding="UTF-8"?>
<template>
	<id>T0000008</id>
	<name>供应商账号初始化导入模板</name>
	<desc>供应商账号初始化导入模板</desc>
	<!--export 保留字段 -->
	<type>import</type>
	<!--XML,Text，html,pdf,csv -->
	<temptype>Excel</temptype>
	<!--Export的模板文件，Import中忽略此属性 -->
	<tempfile>供应商账号初始化导入模板.xlsx</tempfile>
	<!--解析初始行，xml中忽略此属性 -->
	<startrow>1</startrow>
	<!--区分数据间隔，用于Text文本中 ，Excel xml中忽略此属性-->
	<separator>,</separator>
	<!--自定义逻辑,验证导入数据 -->
	<reformatclass></reformatclass>	
	<head name="Sheet1">
		<mfield key="1,1" isnull="true" isunique="true" desc="所属成员企业编号" type="java.lang.String" defaultvalue="">buyerId</mfield>
	</head>
	<mapping name="Sheet1" beginRow="3" endRow="1280" beginColumn="0" endColumn="4">		
			<mfield key="1" isnull="false" isunique="false" desc="营业执照" type="java.lang.String" defaultvalue="">licenceNo</mfield>
			<mfield key="2" isnull="false" isunique="false" desc="账号" type="java.lang.String" defaultvalue="">acNo</mfield>
			<mfield key="3" isnull="false" isunique="false" desc="账户名称" type="java.lang.String" defaultvalue="0">acNm</mfield>
			<mfield key="4" isnull="false" isunique="false" desc="开户行连行号" type="java.lang.String" defaultvalue="0">acBkNo</mfield>
			<mfield key="5" isnull="false" isunique="false" desc="开户行行名" type="java.lang.String" defaultvalue="0">acBkNm</mfield>
	</mapping>
</template>
