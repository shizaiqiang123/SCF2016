<?xml version="1.0" encoding="UTF-8"?>
<template>
	<id>T0000007</id>
	<name>供应商信息初始化导入模板</name>
	<desc>供应商信息初始化导入模板</desc>
	<!--export 保留字段 -->
	<type>import</type>
	<!--XML,Text，html,pdf,csv -->
	<temptype>Excel</temptype>
	<!--Export的模板文件，Import中忽略此属性 -->
	<tempfile>供应商信息初始化导入模板.xlsx</tempfile>
	<!--解析初始行，xml中忽略此属性 -->
	<startrow>1</startrow>
	<!--区分数据间隔，用于Text文本中 ，Excel xml中忽略此属性-->
	<separator>,</separator>
	<!--自定义逻辑,验证导入数据 -->
	<reformatclass></reformatclass>	
	<head name="Sheet1">
		<mfield key="1,1" isnull="true" isunique="true" desc="所属成员企业编号" type="java.lang.String" defaultvalue="">buyerId</mfield>
	</head>
	<mapping name="Sheet1" beginRow="3" endRow="1280" beginColumn="0" endColumn="9">		
			<mfield key="1" isnull="false" isunique="false" desc="营业执照" type="java.lang.String" defaultvalue="">licenceNo</mfield>
			<mfield key="2" isnull="false" isunique="false" desc="ERP编号" type="java.lang.String" defaultvalue="">erpId</mfield>
			<mfield key="3" isnull="false" isunique="false" desc="组织机构号" type="java.lang.String" defaultvalue="">custInstCd</mfield>
			<mfield key="4" isnull="false" isunique="false" desc="公司名称" type="java.lang.String" defaultvalue="0">custNm</mfield>
			<mfield key="5" isnull="false" isunique="false" desc="法人代表" type="java.lang.String" defaultvalue="0">legalRepNm</mfield>
			<mfield key="6" isnull="false" isunique="false" desc="法人电话" type="java.lang.String" defaultvalue="">legalMobPhone</mfield>
			<mfield key="7" isnull="false" isunique="false" desc="联系人姓名" type="java.lang.String" defaultvalue="">contactPerson</mfield>
			<mfield key="8" isnull="false" isunique="false" desc="联系人电话" type="java.lang.String" defaultvalue="">mobPhone</mfield>
			<mfield key="9" isnull="false" isunique="false" desc="邮箱" type="java.lang.String" defaultvalue="">email</mfield>
			<mfield key="10" isnull="false" isunique="false" desc="地址" type="java.lang.String" defaultvalue="">regAddr</mfield>
	</mapping>
</template>
